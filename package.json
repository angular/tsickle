{
  "name": "tsickle",
  "version": "0.32.0",
  "description": "Transpile TypeScript code to JavaScript with Closure annotations.",
  "main": "src/tsickle.js",
  "typings": "src/tsickle.d.ts",
  "bin": "src/main.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "src/*"
  ],
  "dependencies": {
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "source-map": "^0.6.0"
  },
  "peerDependencies": {
    "typescript": ">=2.4.2 <2.9"
  },
  "devDependencies": {
    "@types/glob": "5.0.35",
