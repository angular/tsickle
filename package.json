    "Rado Kirov <radokirov@google.com> (https://angular.io/)",
    "Thomas Deegan <tdeegan@google.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angular/tsickle/issues"
  },
  "homepage": "https://github.com/angular/tsickle",
  "dependencies": {
    "@types/minimist": "^1.2.1"
  }
