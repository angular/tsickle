    "@types/chai": "^3.4.32",
    "@types/diff": "^3.2.0",
    "@types/glob": "^5.0.29",
    "@types/google-closure-compiler": "0.0.18",
    "@types/jasmine": "^2.6.0",
    "@types/minimatch": "^2.0.28",
    "@types/minimist": "^1.1.28",
    "@types/mkdirp": "^0.3.28",
    "@types/node": "^6.0.38",
